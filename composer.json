{
    "name": "xideapl/person-bundle",
    "type": "symfony-bundle",
    "description": "Symfony XideaPersonBundle",
    "keywords": [],
    "homepage": "http://xidea.pl",
    "authors": [
        {
            "name": "Artur Pszczółka",
            "email": "artur.pszczolka@xidea.pl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "url": "https://github.com/XideaPL/Person.git",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.0.*",
        "twig/twig": "~1.23|~2.0",
        "doctrine/doctrine-bundle": "~1.5",
        "doctrine/orm": "~2.4",
        "symfony/assetic-bundle": "~2.3",
        "xideapl/person": "dev-master@dev",
        "xideapl/base-bundle": "dev-master@dev"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "xideapl/user-bundle": "dev-master@dev"
    },
    "autoload": {
        "psr-4": { "Xidea\\Bundle\\PersonBundle\\": "" }
    },
    "target-dir": "Xidea/Bundle/PersonBundle",
	"extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}